        {{ drupal_block('views_exposed_filter_block:frontpage-page_1') }}
      <div class="search-icon">
          {{ drupal_block('block_content:468fca8f-c4f1-46cd-a66e-063c5397c29b') }}
      </div>
    </div> #}
  </div>
{% endif %}
